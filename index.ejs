</head>

<body>


</body>

<script>

	var jokr = new Jokr().play('bootstrap');

	var user_form = new UserProfile('body').create();

	$(document).on('loginSubmitted', handleLogin);

	function handleLogin() {
		if (user_form.isPasswordEmpty() == true ) { 
			Alert(['Password is empty!']); 
		} else if (user_form.isLessThanX(8)) {
			Alert('Password is less than 8 characters.');
		}
	}
</script>

</html>